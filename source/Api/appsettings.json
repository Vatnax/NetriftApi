{
  "Serilog": {
    "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console", "Serilog.Expressions"],

    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning"
      }
    }
  },

  "AllowedHosts": "*"
}
